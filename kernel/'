#include "launch_cmd.h"
#include "struct.h"

static void	my_launch_cmd(t_cmd *cmd, t_cmd *next)
{
	if (next != NULL)
	{
		launch_one_cmd(cmd, next);
	}
}

void		launch_cmd(t_list *cmd)
{
	t_cmd		*prvs;

	prvs = NULL;
	while (cmd != NULL)
	{
		prvs = (t_cmd *)cmd->elem;
		cmd = cmd->next;
	}
}
